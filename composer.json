{
    "name": "omh/laracontact",
    "description": "A Contact Us form for Laravel application",
    "keywords": [
        "laravel",
        "contact form"
    ],
    "homepage": "https://github.com/himan72/laracontact",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed",
            "email": "mohammed@medhicham.com",
            "homepage": "http://medhicham.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "guzzlehttp/guzzle": "^6.5"
    },

    "require-dev": {
        "orchestra/testbench": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Laracontact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laracontact\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Laracontact\\LaracontactServiceProvider"
            ]
        }
    }
}
